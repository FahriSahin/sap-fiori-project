{
  "_version": "1.76.0",
  "sap.app": {
    "id": "project2",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.5",
      "toolsId": "d1a0f8d4-9936-4210-953c-7fbe9d6f4470"
    },
    "dataSources": {
      "mainService": {
        "uri": "/V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.139.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project2.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
   "routing": {
  "config": {
    "routerClass": "sap.m.routing.Router",
    "controlAggregation": "pages",
    "controlId": "app",
    "transition": "slide",
    "type": "View",
    "viewType": "XML",
    "path": "project2.view",
    "async": true,
    "viewPath": "project2.view"
  },
  "routes": [
    {
      "name": "RouteView0",
      "pattern": ":?query:",
      "target": [
        "TargetView0"
      ]
    },
    {
      "name": "RouteView1",
      "pattern": "Product",
      "target": [
        "TargetView1"
      ]
    },
    {
      "name": "RouteView3",
      "pattern": "Employee",
      "target": [
        "TargetViewEmployee"
      ]
    },
    {
      "name":"DetailView",
      "pattern": "Product/{ProductID}",
      "target" : ["TargetView2"]
    },
    {
      "name":"EmployeeDetail",
      "pattern": "EmployeeDetail/{EmployeeID}",
      "target" : ["EmployeeDetail"]
    }
  ],
  "targets": {
    "TargetView0": {
      "id": "Main",
      "name": "Main"
    },
    "TargetView1": {
      "id": "View1",
      "name": "View1"
    },
    "TargetView2":{
      "id":"DetailView",
      "name": "DetailView"
    },
    "TargetViewEmployee":{
      "id":"Employees",
      "name": "Employees"
    },
    "EmployeeDetail":{
      "id":"EmployeeDetail",
      "name": "EmployeeDetail"
    }
  }
},
    "rootView": {
      "viewName": "project2.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
